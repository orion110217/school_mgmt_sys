<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= pageName %> | PES</title>

    <!-- CSS files -->
    <% include partials/template/cssdefaults %>
  </head>
  <body id = <%= pageID %> class = "index-page">
    <!-- Navbar -->
    <% include partials/template/header %>

    <div class="wrapper">
      <div class="main main-raised">
        <div class="section section-basic">
  	    	<div class="container">
              <% teachers.forEach(function(item) { %>
                <div class="row">
                  <div class="col-sm-4">
                    <img src="images/<%= item.shortname %>_tn.jpg" alt="<%= item.shortname %>" class="img-rounded">
                    <h3 class=""><a href="/faculty/<%= item.shortname %>"><%= item.name %></h3></a>
                    <h5 class=""><%= item.title %></h5>
                  </div>
                  <div class="col-sm-8">
                    <h3><%= item.summary %></h3>
                  </div>
                </div>
                <hr>
              <% }); %>
          </div>
        </div>
      </div>
    </div>


    <!-- JS Files -->
    <% include partials/template/jsdefaults %>
  </body>
</html>
